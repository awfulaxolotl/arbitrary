{
  "name": "tastes",
  "version": "0.1.0",
  "description": "Code and create with generative samples",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/awfulaxolotl/tastes",
  "author": "Kevin Sullivan <kevin.sullivan@axolotl.industries>",
  "license": "MIT",
  "scripts": {
    "develop": "tsc && concurrently 'tsc --watch' 'ava --watch'",
