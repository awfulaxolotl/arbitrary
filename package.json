    "@types/react-native": "^0.56.0",
    "@types/storybook__react": "^3.0.5",
    "ava": "1.0.0-beta.4",
    "concurrently": "^3.5.0",
    "react-dom": "^16.1.1",
    "ts-loader": "^4.3.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
