{
  "name": "arbitrary",
  "version": "0.0.0",
  "description": "Generate arbitrary data with control",
  "main": "dist/index.js",
  "files": ["dist"],
  "repository": "https://github.com/awfulaxolotl/arbitrary",
  "author": "Kevin Sullivan <kevin.sullivan@axolotl.industries>",
  "license": "MIT",
  "scripts": {
    "develop":
      "tsc && concurrently 'tsc --watch' 'ava --watch' 'npm run storybook'",
    "storybook": "start-storybook --port 9001 --config-dir .storybook",
    "test": "tsc && ava",
    "build": "npm run build:dist && npm run build:docs",
    "build:dist": "tsc --project tsconfig.dist.json",
    "build:docs": "build-storybook --config-dir .storybook --output-dir docs"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-primitives": "^0.5.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.16",
    "@types/ramda": "^0.25.5",
    "@types/react-native": "^0.52.9",
    "@types/storybook__react": "^3.0.5",
    "ava": "^0.25.0",
    "concurrently": "^3.5.0",
    "react-dom": "^16.1.1",
    "ts-loader": "^3.1.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
